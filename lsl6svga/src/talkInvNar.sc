(instance talkInvNar of LarryTalker
	(properties)
	
	(method (init)
		(= font userFont)
		(super init: &rest)
	)
	
	(method (dispose)
		(Print title: 0)
		(super dispose: &rest)
	)
	
	(method (display theText &tmp theTalkWidth newPrint)
		(if (> (+ x talkWidth) 318)
			(= theTalkWidth (- 318 x))
		else
			(= theTalkWidth talkWidth)
		)
		((= newPrint (Print new:))
			addTitle: (global186 data?)
			fore: fore
			back: back
			posn: x y
			font: font
			width: theTalkWidth
			addText: theText
			modeless: 2
			init:
		)
		(prints delete: newPrint)
		(= dialog (newPrint dialog?))
	)
)
